{
  "name": "frontend",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:safe": "bash ../scripts/frontend-health-check.sh && vite",
    "build": "tsc -b && vite build",
    "build:electron": "vite build --base=./",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "bash ../scripts/frontend-health-check.sh",
    "type-check": "tsc --noEmit",
    "test:dictation": "tsc --project tsconfig.test.json && node --test dist-test/test/*.js",
    "clean": "rm -rf node_modules/.vite dist",
    "restart": "pkill -f vite && npm run dev"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "framer-motion": "^12.23.25",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
